{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery.js", "display.js"],
      "css": ["display.css"]
    }
  ],
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
